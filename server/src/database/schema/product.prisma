model Product {
    id          Int        @id @default(autoincrement())
    name        String     @unique
    description String
    ingredients String
    imageUrl    String
    numReviews  Int        @default(0) @map("num_reviews")
    rating      Float      @default(0.0)
    createdAt   DateTime   @default(now()) @map("created_at")
    cartItems   CartItem[]
    orders      Order[]
    packages    Package[]

    @@map("products")
}