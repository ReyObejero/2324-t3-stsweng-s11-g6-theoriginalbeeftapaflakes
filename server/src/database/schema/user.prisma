model User {
    id            Int            @id @default(autoincrement())
    username      String         @unique
    email         String         @unique
    password      String
    role          UserRole       @default(USER)
    cart          Cart?
    refreshTokens RefreshToken[]

    @@map("users")
}

enum UserRole {
    USER
    ADMIN

    @@map("user_role")
}