{
    "version": 2,
    "builds": [
        {
            "src": "./build/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/build/index.js"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [{ "key": "Access-Control-Allow-Origin", "value": "http://localhost:3000" }]
        }
    ]
}
