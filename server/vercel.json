{
    "version": 2,
    "builds": [
        {
            "src": "./build/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/build/index.js",
            "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
            "headers": {
                "Access-Control-Allow-Origin": "http://localhost:3000"
            }
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [{ "key": "Access-Control-Allow-Origin", "value": "http://localhost:3000" }]
        }
    ]
}
